#include <iostream>
using namespace std;
int main()
{
	int x,y,temp;
	cout<<"Enter two numbers:";
	cin>>x>>y;
    cout<<"Before swapping the values are :\n x="<<x<<" and y="<<y<<endl;
	temp=x;
    x=y;
    x=temp;
    cout<<"After swapping the values are :\n x="<<x<<" and y="<<y<<endl;
	return 0;
}
